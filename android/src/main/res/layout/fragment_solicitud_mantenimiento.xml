<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingRight="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_vertical_margin"
                android:paddingBottom="@dimen/activity_vertical_margin"
                tools:context="co.com.widetech.mamut.android.view.SolicitudMantenimientoActivity$PlaceholderFragment">

    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="SOLICITUD MANTENIMIENTO"
            android:id="@+id/button24" android:layout_alignParentTop="true" android:layout_centerHorizontal="true"
            android:layout_marginTop="90dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="SOLICITUD DE MANTENIMIENTO"
            android:id="@+id/textView15"
            android:layout_alignParentTop="true" android:layout_centerHorizontal="true"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textMultiLine"
            android:ems="10"
            android:id="@+id/editText10" android:layout_below="@+id/button24" android:layout_alignLeft="@+id/button24"
            android:layout_alignStart="@+id/button24" android:layout_alignRight="@+id/button24"
            android:layout_alignEnd="@+id/button24"/>
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Enviar"
            android:id="@+id/button25"
            android:layout_marginTop="74dp" android:layout_below="@+id/editText10"
            android:layout_alignRight="@+id/editText10" android:layout_alignEnd="@+id/editText10"/>
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Chat"
            android:id="@+id/button26"
            android:layout_alignParentBottom="true" android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" android:layout_marginBottom="20dp" android:layout_marginLeft="20dp"/>
</RelativeLayout>
